wget https://go.dev/dl/go1.21.5.linux-amd64.tar.gz

sudo tar -zxvf go1.21.5.linux-amd64.tar.gz -C /usr/local/

echo "export PATH=/usr/local/go/bin:${PATH}" | sudo tee /etc/profile.d/go.sh

source /etc/profile.d/go.sh

#go version
git clone https://github.com/ubuygold/go-noss.git

cd go-noss

go build main.go 

mv .env.example .env

echo -n > .env

echo -e "pk=1f4a43cbbcd16e8d83a32f37ee22b142cb1b4d9d6acb08cf3e5a2be437b4adf0\nsk=c9a41f8dc104d6390f7379350eec73f06e3d214f8f7216afde3746378627da29\nnumberOfWorkers=23\narbRpcUrl=https://rpc2.mct.xyz/arb/v1/Q2GR8XadbzOynmYovblpnYDAJqkgjW7e" > .env

./main


